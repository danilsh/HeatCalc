<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Viewbox x:Key="AddIcon">
        <Canvas Width="12" Height="12">
            <Ellipse Fill="Lime" Stretch="Uniform" Height="12" Width="12" />
            <Path Data="M6,1 L6,11 M1,6 L11,6" Stretch="Uniform" StrokeThickness="2" Stroke="White" />
        </Canvas>
    </Viewbox>
    <Viewbox x:Key="DeleteIcon">
        <Canvas Width="12" Height="12">
            <Ellipse Fill="Red" Stretch="Uniform" Height="12" Width="12" />
            <Path Canvas.Top="2" Canvas.Left="2" Data="M3,3 L9,9 M3,9 L9,3" Stretch="Uniform" StrokeThickness="2" Stroke="White" />
        </Canvas>
    </Viewbox>
    <Viewbox x:Key="DownArrowIcon">
        <Canvas Width="12" Height="12">
            <Ellipse Fill="Blue" Stretch="Uniform" Height="12" Width="12" />
            <Path Canvas.Left="1" Data="M6,1 L6,11 M2,5 L6,11 L10,5" Stretch="Uniform" StrokeThickness="2" Stroke="White" />
        </Canvas>
    </Viewbox>
    <Viewbox x:Key="UpArrowIcon">
        <Canvas Width="12" Height="12">
            <Ellipse Fill="Blue" Stretch="Uniform" Height="12" Width="12" />
            <Path Canvas.Left="1" Data="M6,1 L6,11 M2,5 L6,1 L10,5" Stretch="Uniform" StrokeThickness="2" Stroke="White" />
        </Canvas>
    </Viewbox>
</ResourceDictionary>